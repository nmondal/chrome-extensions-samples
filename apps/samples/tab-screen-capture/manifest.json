{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "assets/128.png",
      "default_title": "Take Screenshot"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+P"
         }
      }
   },
   "description": "Take screenshot of current tab with Notes. Keyboard Command: Alt+P.",
   "homepage_url": "https://nmondal.github.io",
   "icons": {
      "128": "assets/128.png",
      "48": "assets/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Screenshot",
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "downloads" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.5"
}
